<center>
    <h3>Your favoites photos</h3>
</center>
<div class="photo_image_each">
    <% @likes.each do |like| %>
        <% photo = Photo.find_by(id: like.photo_id) %>
           <%= link_to public_photo_path(photo) do  %>
                    <%= attachment_image_tag photo,:image,fallback: "noimage.jpg", style:'width: 30%;height:33%;padding:3%;' %>
                <% end %>
    <% end %>
</div>
