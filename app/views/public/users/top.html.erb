    <% if user_signed_in? %>
             <div class="recommend_photographer">
                <div class="recommend_item_each">
                    <% @photos.each do |photo|%>
                        <%= link_to public_photo_path(photo) do  %>
                            <%= attachment_image_tag photo,:image,fallback: "noimage.jpg", style:'width: 33%;height:33%;padding:10px;' %>
                        <% end %>
                    <% end%>
                </div>
              </div>
        <% else %>
            <%= link_to"let's explore photos",  new_user_registration_path ,class:"btn btn-primary"%>
                <%3.times do |i|%>
                    <div class="recommend_item_each"> 
                        <%= link_to public_photo_path(@photos[i].id) do %>
                            <%= attachment_image_tag @photos[i],:image, fallback: "noimage.png", style:'width: 33%;height:33%;padding:10px;'%>
                        <% end %>
                    </div> 
                <%end %>

    <% end %>
