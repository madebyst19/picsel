<h1>ユーザー詳細ページ</h1>
<h3><%= @user.email %></h3>
<h2>投稿内容</h2>
<% @user.photos.each do |photo| %>
  <a href="/posts/<%= photo.id %>"><%= photo.title %></a>
  <hr>
<% end %>
<h3>いいねしている投稿</h3>
<% @user.liked_photos.each do |photo| %>
  <%= photo.user.email %>
  <%= photo.title %>
<% end %>
<%= link_to "ユーザー一覧へ", root_path %>