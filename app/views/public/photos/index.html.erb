<div class="user_name">
<%#= @photo.user.first_name %>
    <center>
        <h3>TARO</h3>
    </center>
    <%= link_to"add",new_public_photo_path,style:"margin-left:560px;" %>
</div>
<div class="photo_index_images">
    <% @photos.each do |photo| %>
        <%= link_to public_photo_path(photo) do  %>
            <%= attachment_image_tag photo,:image,fallback: "noimage.jpg", size:'100x100' %>
        <% end %>
    <% end %>
</div>